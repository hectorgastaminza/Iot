<?xml version="1.0" encoding="UTF-8"?>
<!-- webapp/WEB-INF/web.xml -->
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>To do List</display-name>
	<welcome-file-list>
		<welcome-file>login.do</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/WEB-INF/views/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>signup</servlet-name>
		<jsp-file>/WEB-INF/views/signup.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signup/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>recovery</servlet-name>
		<jsp-file>/WEB-INF/views/recovery.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>recovery</servlet-name>
		<url-pattern>/recovery/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>home</servlet-name>
		<jsp-file>/WEB-INF/views/home.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>userconfig</servlet-name>
		<jsp-file>/WEB-INF/views/userconfig.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>userconfig</servlet-name>
		<url-pattern>/userconfig/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>connection</servlet-name>
		<jsp-file>/WEB-INF/views/connection.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>connection</servlet-name>
		<url-pattern>/connection/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>devicecreate</servlet-name>
		<jsp-file>/WEB-INF/views/devicecreate.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>devicecreate</servlet-name>
		<url-pattern>/devicecreate/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>placecreate</servlet-name>
		<jsp-file>/WEB-INF/views/placecreate.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>placecreate</servlet-name>
		<url-pattern>/placecreate/*</url-pattern>
	</servlet-mapping>
</web-app>